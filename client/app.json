      "package": "com.nirat.client"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "36a56eb6-64c3-485e-a0e5-bb608cf560cc"
      }
    },
